@model IEnumerable<Question_2_MVC.Models.Order>

<h2>Order Details</h2>

<table style="border-collapse: collapse; width: 100%;" border="1">
    <tr>
        <th>Order ID</th>
        <th>Order Date</th>
        <th>Shipment Country</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.OrderID</td>
            <td>@item.OrderDate</td>
        </tr>
    }
</table>

